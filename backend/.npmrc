# Increase Prisma migration timeouts for Neon PostgreSQL
PRISMA_QUERY_ENGINE_LIBRARY_ADVISORY_LOCK_TIMEOUT=60000
PRISMA_MIGRATION_ADVISORY_LOCK_TIMEOUT=60000
PRISMA_CLIENT_ENGINE_LIBRARY_ADVISORY_LOCK_TIMEOUT=60000

# Increase network timeout
network-timeout=100000
fetch-timeout=100000
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
